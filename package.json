{
  "private": true,
  "name": "geo-ball",
  "license": "MIT",
  "author": "Joshua Ball <joshua.g.s.ball@gmail.com>",
  "repository": "git@github.com:joshball/geo-ball.git",
  "version": "1.0.0",
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/@storybook",
      "**/@storybook/**",
      "**/electron",
      "**/electron/**",
      "**/electron-builder",
      "**/electron-builder/**",
      "**/electron-webpack",
      "**/electron-webpack/**"
    ]
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build:stream": "lerna run build --stream",
    "build:clean": "lerna run --parallel build:clean",
    "build:watch": "lerna run --parallel build:watch",
    "build": "lerna run build",
    "clean:node_modules": "lerna run --parallel clean:node_modules",
    "clean:react": "rimraf node_modules/fannypack/node_modules/@types/react",
    "clean": "lerna run clean",
    "cleanup:packages": "yarn run prettier-package-json --write && lerna run cleanup:packages",
    "electron": "lerna run electron --scope ucsd-app",
    "package": "lerna run package --scope ucsd-app --stream",
    "preelectron": "yarn build",
    "prepackage": "yarn build",
    "postinstall": "yarn clean:react",
    "start": "lerna exec --parallel --stream yarn start",
    "test": "lerna run test",
    "build:cl": "parcel build ./packages/component-lib/src/index.ts --no-cache --out-dir ./packages/component-lib/build-parcel"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^3",
    "@types/electron-is-dev": "^0.3.0",
    "@types/electron-store": "^1.3.0",
    "@types/mousetrap": "^1.6.0",
    "@types/node": "^10.11.7",
    "@typescript-eslint/eslint-plugin": "^1.x",
    "@typescript-eslint/eslint-plugin-tslint": "^1.4.2",
    "@typescript-eslint/parser": "^1.x",
    "ava": "^0.25.0",
    "ava-ts": "^0.25.2",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "coveralls": "^3.0.2",
    "electron-builder": "^20.38.4",
    "eslint": "^5.x",
    "eslint-config-prettier": "^4.x",
    "eslint-plugin-eslint-plugin": "^2.x",
    "eslint-plugin-import": "^2.x",
    "eslint-plugin-jest": "^22.x",
    "eslint-plugin-jsx-a11y": "^6.x",
    "eslint-plugin-markdown": "^1.x",
    "eslint-plugin-prettier": "^3.x",
    "eslint-plugin-react": "^7.x",
    "eslint-plugin-react-hooks": "^1.5.1",
    "globby": "^8",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-expect-contain-deep": "^1.0.1",
    "lerna": "^3.4.3",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.16.4",
    "prettier-package-json": "^2.0.1",
    "react-powerplug": "^1.0.0",
    "react-test-renderer": "^16.7.0",
    "reakit-theme-default": "^0.2.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.2",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-sane": "^1.0.4",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@geo-ball/api-couturier": "^1.0.0",
    "@geo-ball/component-lib": "^1.0.0",
    "@geo-ball/geo-core": "^1.0.0",
    "@geo-ball/graph-core": "^1.0.0",
    "@geo-ball/osm-cli": "^1.0.0",
    "@geo-ball/osm-data": "^1.0.0",
    "@geo-ball/ucsd-app": "^1.0.0",
    "@geo-ball/ucsd-cli": "^1.0.0",
    "@geo-ball/ucsd-core": "^1.0.0",
    "@geo-ball/utils": "^1.0.0"
  }
}
