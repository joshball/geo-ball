{
  "private": true,
  "name": "ball-maps",
  "license": "MIT",
  "author": "Joshua Ball <joshua.g.s.ball@gmail.com>",
  "repository": "git@github.com:joshball/ball-maps.git",
  "version": "1.0.0",
  "workspaces": {
    "packages": ["packages/*"],
    "nohoist": [
      "**/@storybook",
      "**/@storybook/**",
      "**/electron",
      "**/electron/**",
      "**/electron-builder",
      "**/electron-builder/**",
      "**/electron-webpack",
      "**/electron-webpack/**"
    ]
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build:stream": "lerna run build --stream",
    "build:watch": "lerna run --parallel build:watch",
    "build": "lerna run build",
    "clean:node_modules": "lerna run clean:node_modules",
    "clean": "lerna run clean",
    "cleanup:packages": "yarn run prettier-package-json --write && lerna run cleanup:packages",
    "electron": "lerna run electron --scope ucsd-app",
    "package": "lerna run package --scope ucsd-app --stream",
    "preelectron": "yarn build",
    "prepackage": "yarn build",
    "start": "lerna exec --parallel --stream yarn start",
    "test": "lerna run test"
  },
  "devDependencies": {
    "@types/electron-is-dev": "^0.3.0",
    "@types/electron-store": "^1.3.0",
    "@types/mousetrap": "^1.6.0",
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^3",
    "@types/node": "^10.11.7",
    "ava": "^0.25.0",
    "ava-ts": "^0.25.2",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.2",
    "electron-builder": "^20.38.4",
    "fuse-box": "^3.6.0",
    "globby": "^8",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-expect-contain-deep": "^1.0.1",
    "lerna": "^3.4.3",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "prettier-package-json": "^2.0.1",
    "react-powerplug": "^1.0.0",
    "react-test-renderer": "^16.7.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "tslint-plugin-prettier": "^2.0.0",
    "tslint-sane": "^1.0.4",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@ball-maps/geo-core": "^1.0.0",
    "@ball-maps/graph-core": "^1.0.0",
    "@ball-maps/osm-cli": "^1.0.0",
    "@ball-maps/osm-data": "^1.0.0",
    "@ball-maps/ucsd-cli": "^1.0.0",
    "@ball-maps/ucsd-core": "^1.0.0",
    "@ball-maps/ucsd-app": "^1.0.0",
    "@ball-maps/utils": "^1.0.0"
  }
}
