{
  "name": "@ball-maps/ucsd-app",
  "productName": "UCSD Map App",
  "main": "out/main.js",
  "version": "1.0.0",
  "homepage": "homepage",
  "description": "An electron starter project.",
  "license": "MIT",
  "private": true,
  "build": {
    "appId": "com.joshuaball.ball-maps",
    "files": [
      "!src",
      "!docs",
      "!tsconfig.json",
      "!fuse.js",
      "!dev-app-update.yml",
      "!README.md",
      "!LICENSE",
      "!globals.d.ts"
    ],
    "linux": {
      "target": [
        "tar.gz"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "publish": {
      "owner": "joshball",
      "provider": "github",
      "repo": "ball-maps"
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ]
    }
  },
  "scripts": {
    "build-storybook": "build-storybook",
    "clean": "rm -rf .fusebox out dist coverage",
    "clean:node_modules": "rimraf node_modules",
    "cleanup:packages": "yarn run prettier-package-json --write",
    "compile": "NODE_ENV=production ts-node fuse",
    "coverage:upload": "cat ./coverage/lcov.info | coveralls",
    "coverage": "yarn test --coverage --no-cache",
    "dist": "electron-builder --mac --win --linux --ia32 --x64",
    "draft": "electron-builder -p always --draft --mac --win --linux --ia32 --x64",
    "format": "prettier --write **/*.ts && tslint -p . --fix",
    "lint": "tslint --project .",
    "build": "electron-builder --dir",
    "pack": "electron-builder --dir",
    "postinstall": "electron-builder install-app-deps",
    "precommit": "lint-staged",
    "prepush": "npm-run-all typecheck lint",
    "sanity": "npm-run-all clean typecheck lint test clean compile clean",
    "start": "ts-node fuse",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc -p . --noEmit --pretty",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save",
    "Xprebuild": "copyfiles -u 2 \"../crale-react/build/**/*\" static/ && ./scripts/fixElectronBug.sh",
    "Xbuild": "electron-webpack",
    "electron": "electron dist/main/main.js",
    "Xprepackage": "./scripts/revertElectronBug.sh",
    "package": "electron-builder",
    "Xstart:wp": "cross-env ELECTRON_START_URL=http://localhost:3000 electron-webpack dev"
  },
  "dependencies": {
    "@ball-maps/geo-core": "^1.0.0",
    "@ball-maps/osm-data": "^1.0.0",
    "@ball-maps/ucsd-core": "^1.0.0",
    "@blueprintjs/core": "^3.11.0",
    "@blueprintjs/icons": "^3.5.0",
    "@blueprintjs/select": "^3.5.0",
    "electron-is-dev": "^1.0.1",
    "electron-log": "^2.2.17",
    "electron-store": "^2.0.0",
    "electron-updater": "^4.0.6",
    "electron-window-state-manager": "^0.3.2",
    "glamor": "^2.20.40",
    "leaflet": "^1.4.0",
    "leaflet-geosearch": "^2.7.0",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "mobx-react-devtools": "^6.0.3",
    "mousetrap": "^1.6.2",
    "normalize.css": "^8.0.1",
    "popmotion": "^8.5.5",
    "popmotion-react": "^2.1.1",
    "prop-types": "^15.6.2",
    "ramda": "^0.26.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-leaflet": "^2.1.4",
    "react-transition-group": "^2.5.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addon-storyshots": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "@types/electron-is-dev": "^0.3.0",
    "@types/electron-store": "^1.3.0",
    "@types/jest": "^23.3.12",
    "@types/moment-timezone": "^0.5.10",
    "@types/mousetrap": "^1.6.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "@types/storybook__react": "^4.0.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.2",
    "electron": "^4.0.0",
    "electron-builder": "^20.38.4",
    "fuse-box": "^3.6.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-expect-contain-deep": "^1.0.1",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "react-powerplug": "^1.0.0",
    "react-test-renderer": "^16.7.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.ts"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>../../node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mock-file.ts",
      "\\.(css|less)$": "<rootDir>/test/mock-style.ts"
    },
    "testRegex": "(/__tests__/.*|\\.(spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "./node_modules",
      "./out",
      "./build",
      "./dist",
      "./test",
      "./docs",
      "\\.story.tsx$"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": false
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
