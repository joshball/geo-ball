{
  "name": "@geo-ball/ucsd-app",
  "productName": "UCSD Map App",
  "main": "out/main.js",
  "version": "1.0.0",
  "homepage": "homepage",
  "description": "A UCSD Mapping application.",
  "author": "Joshua Ball <joshua.g.s.ball@gmail.com>",
  "license": "MIT",
  "private": true,
  "build": {
    "appId": "com.joshuaball.geo-ball",
    "files": [
      "!src",
      "!docs",
      "!tsconfig.json",
      "!fuse.js",
      "!dev-app-update.yml",
      "!README.md",
      "!LICENSE",
      "!globals.d.ts"
    ],
    "linux": {
      "target": [
        "tar.gz"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "publish": {
      "owner": "joshball",
      "provider": "github",
      "repo": "geo-ball"
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ]
    }
  },
  "_moduleAliases": {
    "@root": [
      "../componet-lib/src"
    ]
  },
  "scripts": {
    "build-storybook": "build-storybook",
    "clean": "rm -rf .fusebox out dist coverage .nyc_output",
    "clean:node_modules": "rimraf node_modules",
    "cleanup:packages": "yarn run prettier-package-json --write",
    "compile": "NODE_ENV=production ts-node fuse",
    "coverage:upload": "cat ./coverage/lcov.info | coveralls",
    "coverage": "yarn test --coverage --no-cache",
    "dist": "electron-builder --mac --win --linux --ia32 --x64",
    "draft": "electron-builder -p always --draft --mac --win --linux --ia32 --x64",
    "format": "prettier --write **/*.ts && tslint -p . --fix",
    "lint": "tslint --project .",
    "build": "electron-builder --dir",
    "build:clean": "tsc --build tsconfig.json --clean",
    "pack": "electron-builder --dir",
    "postinstall": "electron-builder install-app-deps",
    "precommit": "lint-staged",
    "prepush": "npm-run-all typecheck lint",
    "sanity": "npm-run-all clean typecheck lint test clean compile clean",
    "start": "ts-node fuse",
    "start:electron": "electron .",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc -p . --noEmit --pretty",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save",
    "Xprebuild": "copyfiles -u 2 \"../crale-react/build/**/*\" static/ && ./scripts/fixElectronBug.sh",
    "Xbuild": "electron-webpack",
    "electron": "electron dist/main/main.js",
    "Xprepackage": "./scripts/revertElectronBug.sh",
    "package": "electron-builder",
    "Xstart:wp": "cross-env ELECTRON_START_URL=http://localhost:3000 electron-webpack dev"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.x",
    "@blueprintjs/icons": "^3.x",
    "@blueprintjs/select": "^3.x",
    "@geo-ball/api-couturier": "^1.0.0",
    "@geo-ball/component-lib": "^1.0.0",
    "@geo-ball/geo-core": "^1.0.0",
    "@geo-ball/osm-data": "^1.0.0",
    "@geo-ball/ucsd-core": "^1.0.0",
    "@geo-ball/utils": "^1.0.0",
    "@turf/turf": "^5.x",
    "body-parser": "^1.x",
    "date-fns": "^2.0.0-alpha.27",
    "electron-better-ipc": "^0.1.2",
    "electron-context-menu": "^0.11.0",
    "electron-is-dev": "^1.0.1",
    "electron-log": "^3.0.1",
    "electron-store": "^2.0.0",
    "electron-updater": "^4.0.6",
    "electron-window-state-manager": "^0.3.2",
    "express": "^4.x",
    "formik": "^1.x",
    "glamor": "^2.x",
    "informed": "^2.x",
    "leaflet": "^1.x",
    "leaflet-geosearch": "^2.x",
    "mobx": "^5.x",
    "mobx-react": "^5.x",
    "mousetrap": "^1.x",
    "normalize.css": "^8.x",
    "prop-types": "^15.x",
    "ramda": "^0.x",
    "react": "^16.x",
    "react-dom": "^16.x",
    "react-leaflet": "^2.x",
    "react-router": "^4.x",
    "react-router-dom": "^4.x",
    "react-transition-group": "^2.x",
    "uuid": "^3.x"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.x",
    "@storybook/addon-info": "^4.x",
    "@storybook/addon-links": "^4.x",
    "@storybook/addon-storyshots": "^4.x",
    "@storybook/cli": "^4.x",
    "@storybook/react": "^4.x",
    "@types/body-parser": "^1.x",
    "@types/dom4": "^2.x",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/electron-is-dev": "^0.3.0",
    "@types/electron-store": "^1.3.0",
    "@types/express": "^4.x",
    "@types/jest": "^24.x",
    "@types/moment-timezone": "^0.x",
    "@types/mousetrap": "^1.x",
    "@types/react": "^16.x",
    "@types/react-dom": "^16.x",
    "@types/react-leaflet": "^1.x",
    "@types/react-router": "^4.x",
    "@types/react-router-dom": "^4.x",
    "@types/react-test-renderer": "^16.x",
    "@types/storybook__react": "^4.x",
    "@types/uuid": "^3.x",
    "awesome-typescript-loader": "^5.x",
    "coveralls": "^3.x",
    "devtron": "^1.x",
    "electron": "^4.0.3",
    "electron-builder": "^20.38.4",
    "electron-devtools-installer": "^2.2.4",
    "fuse-box": "^3.x",
    "husky": "^1.x",
    "jest": "^24.x",
    "jest-expect-contain-deep": "^1.x",
    "lint-staged": "^8.x",
    "module-alias": "^2.x",
    "node-sass": "^4.x",
    "npm-run-all": "^4.x",
    "postcss-modules": "^1.x",
    "prettier": "^1.x",
    "react-devtools": "^3.x",
    "react-docgen-typescript-loader": "^3.x",
    "react-docgen-typescript-webpack-plugin": "^1.x",
    "react-powerplug": "^1.x",
    "react-router-dom": "^4.x",
    "react-test-renderer": "^16.x",
    "ts-jest": "^23.x",
    "ts-loader": "^5.x",
    "ts-node": "^8.x",
    "tslint": "^5.x",
    "tslint-config-prettier": "^1.x",
    "typescript": "^3.x"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.ts"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>../../node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mock-file.ts",
      "\\.(css|less)$": "<rootDir>/test/mock-style.ts"
    },
    "testRegex": "(/__tests__/.*|\\.(spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "./node_modules",
      "./out",
      "./build",
      "./dist",
      "./test",
      "./docs",
      "\\.story.tsx$"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": false
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
