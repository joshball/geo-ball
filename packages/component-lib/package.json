{
  "private": true,
  "name": "@geo-ball/component-lib",
  "description": "Library for React Components",
  "license": "ISC",
  "author": "Joshua Ball <joshua.g.s.ball@gmail.com>",
  "homepage": "https://github.com/joshball/geo-ball#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshball/geo-ball.git"
  },
  "bugs": {
    "url": "https://github.com/joshball/geo-ball/issues"
  },
  "version": "1.0.0",
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "directories": {
    "lib": "build",
    "test": "tests"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:clean": "tsc --build tsconfig.json --clean",
    "build:watch": "tsc -p tsconfig.json --watch",
    "clean": "rimraf build .nyc_output",
    "clean:node_modules": "rimraf node_modules",
    "cleanup:packages": "yarn run prettier-package-json --write",
    "eslint:lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "eslint:check-config": "eslint --print-config . | eslint-config-prettier-check",
    "prettier:format": "prettier-eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier:write": "prettier-eslint --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "sg": "styleguidist server",
    "test": "yarn run build && yarn run nyc ava --verbose",
    "test:watch": "yarn run build && yarn run ava --watch"
  },
  "ava": {
    "snapshotDir": "./snapshots",
    "failFast": true,
    "files": [
      "build/**/*.spec.js"
    ],
    "sources": [
      "build/**/*.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-3": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.2",
    "@types/styled-components": "^4.1.11",
    "@typescript-eslint/parser": "^1.4.2",
    "ava": "^1.1.0",
    "ava-ts": "^0.25.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "create-index": "^2.4.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-eslint-plugin": "^2.0.1",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "nyc": "^13.1.0",
    "prettier": "^1.16.4",
    "react-docgen-typescript": "^1.12.3",
    "react-styleguidist": "^9.0.1",
    "reakit-theme-default": "^0.2.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.13.0",
    "formik": "^1.5.1",
    "glamor": "^2.20.40",
    "json-stringify-safe": "^5.0.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "reakit": "^0.16.0"
  }
}
